{
    "name": "hycon",
    "version": "0.2.1",
    "description": "HYCON Full-node",
    "main": "index.js",
    "bin": "build/src/main.js",
    "pkg": {
        "scripts": "node_modules/stratum/lib/*.js",
        "assets": "mnemonic/*"
    },
    "scripts": {
        "hycon": "./node_modules/.bin/ts-node src/main.ts --port=8148 --str_port=9081 --api --api_port=2442 --cpuMiners=1",
        "nogui": "./node_modules/.bin/ts-node src/main.ts --port=8148 --str_port=9081 --api --api_port=2442 --cpuMiners=1 --noGUI",
        "testnet": "./node_modules/.bin/ts-node src/main.ts --port=8148 --str_port=9081 --api --api_port=2442 --cpuMiners=1 --noGUI --networkid=ouroboros --bootstrap",
        "seed": "./node_modules/.bin/ts-node src/main.ts --port=8148 --str_port=9081 --api --api_port=2442 --cpuMiners=0",
        "protogen": "pbjs -t static-module -w commonjs -o src/serialization/proto.js proto/main.proto &&  pbts -o src/serialization/proto.d.ts src/serialization/proto.js && ./src/serialization/trim_null.sh",
        "debug": "./node_modules/.bin/ts-node --inspect=9231 --inspect-brk src/main.ts  --port=8148 --str_port=9081 --cpuMiners=3 --api --api_port=2442 --networkid=hycon",
        "debugTests": "./node_modules/.bin/ts-node  --inspect=9230 --inspect-brk  test/runner.ts",
        "test": "rm -rf build && tsc && istanbul cover node_modules/jasmine/bin/jasmine.js -x **/proto.*  -- --config=jasmine.json && remap-istanbul -i ./coverage/coverage.json -o ./coverage/remap-report -t html",
        "clear": "rm -rf blockdb* worldstate* rawblock* txDB* peerdb* minedDB*",
        "package": "tsc && pkg package.json",
        "prune": "./node_modules/.bin/ts-node worldstate_copy_root.ts",
        "block:build": "cd ./src/api/client && webpack"
    },
    "author": "Team HYCON",
    "license": "ISC",
